{
    "extends": ["next/core-web-vitals", "airbnb", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/react-in-jsx-scope": "off"
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js"]
            }
        }
    }
}
